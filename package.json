{
    "name": "cargo-monorepo",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "api",
        "client"
    ],
    "scripts": {
        "api": "yarn workspace api build-docker",
        "client": "yarn workspace client start",
        "start": "concurrently --kill-others-on-fail \"yarn api\"  \" yarn client \""
    },
    "devDependencies": {
        "concurrently": "^7.0.0"
    }
}
